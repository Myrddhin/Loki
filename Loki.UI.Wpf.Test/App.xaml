<Application x:Class="Loki.UI.Wpf.Test.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Loki.UI.Wpf.Test">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage UriSource="pack://application:,,,/Loki.UI.Wpf.Test;component/Resources/mni_search.png" x:Key="MNI_search" />
            <BitmapImage UriSource="pack://application:,,,/Loki.UI.Wpf.Test;component/Resources/mni_refresh.png" x:Key="MNI_refresh" />
            <BitmapImage UriSource="pack://application:,,,/Loki.UI.Wpf.Test;component/Resources/mni_save.png" x:Key="MNI_save" />
            <BitmapImage UriSource="pack://application:,,,/Loki.UI.Wpf.Test;component/Resources/mni_export.png" x:Key="MNI_export" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:TestBootStrapper x:Key="bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>